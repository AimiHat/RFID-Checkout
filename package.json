{
  "name": "OcadowCheckout",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Automated checkout system",
  "author": "Aimilios <aimilios@wisepricing.com>",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "main": "main.js",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "build": {
    "appId": "com.example.app",
    "dmg": {
      "icon": "build/icon.icns",
      "background": "build/background.png",
      "contents": [
        {
          "x": 100,
          "y": 100
        },
        {
          "x": 150,
          "y": 100,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "portable",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "electron": "latest",
    "electron-builder": "latest"
  }
}
